{
  "build": {
    "builder": "Nixpacks"
  },
  "deploy": {
    "startCommand": "streamlit run main.py --server.port $PORT --server.address 0.0.0.0"
  },
  "envs": {
    "OPENAI_API_KEY": {
      "required": true
    },
    "TWILIO_ACCOUNT_SID": {
      "required": true
    },
    "TWILIO_AUTH_TOKEN": {
      "required": true
    },
    "TWILIO_PHONE_NUMBER": {
      "required": true
    }
  }
}



